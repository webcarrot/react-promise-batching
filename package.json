{
  "author": {
    "email": "admin@webcarrot.pl",
    "name": "webcarrot"
  },
  "bugs": {
    "url": "https://github.com/webcarrot/react-promise-batching/issues"
  },
  "description": "Batching strategy that support promise as callback",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "esformatter": "^0.9.6",
    "esformatter-jsx": "^7.0.1",
    "esformatter-quotes": "^1.1.0",
    "eslint": "^3.4.0",
    "eslint-plugin-react": "^6.2.0"
  },
  "esformatter": {
    "jsx": {
      "alignWithFirstAttribute": true,
      "attrsOnSameLineAsTag": false,
      "firstAttributeOnSameLine": true,
      "formatJSX": true,
      "htmlOptions": {
      },
      "maxAttrsOnTag": 1,
      "spaceInJSXExpressionContainers": ""
    },
    "plugins": [
      "esformatter-quotes",
      "esformatter-jsx"
    ],
    "quotes": {
      "avoidEscape": false,
      "type": "double"
    }
  },
  "eslintConfig": {
    "ecmaFeatures": {
      "arrowFunctions": true,
      "blockBindings": true,
      "forOf": true,
      "jsx": true,
      "modules": true
    },
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "react"
    ],
    "rules": {
      "no-console": 0,
      "no-underscore-dangle": 0,
      "no-unused-vars": 1,
      "quotes": 0
    }
  },
  "installable": false,
  "jshintConfig": {
    "esnext": true,
    "node": true,
    "strict": true,
    "unused": true
  },
  "keywords": [
    "batching",
    "react",
    "strategy",
    "promise",
    "async"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/webcarrot/react-promise-batching/blob/master/LICENSE"
    }
  ],
  "main": "./BatchingStrategy.js",
  "maintainers": [
    {
      "email": "admin@webcarrot.pl",
      "name": "webcarrot"
    }
  ],
  "name": "react-promise-batching",
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0-rc.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webcarrot/react-promise-batching.git"
  },
  "scripts": {
    "build": "babel source --out-dir distribution",
    "format": "esformatter -i source/**/*.js",
    "lint": "eslint ./source",
    "preinstall": "node ./scripts/preinstall.js",
    "prepublish": "npm run build"
  },
  "version": "0.0.1"
}
