{
  "author": {
    "email": "admin@webcarrot.pl",
    "name": "webcarrot"
  },
  "bugs": {
    "url": "https://github.com/webcarrot/react-promise-batching/issues"
  },
  "description": "Batching strategy that support promise and generators as callback",
  "esformatter": {
    "plugins": [
      "esformatter-quotes"
    ],
    "quotes": {
      "avoidEscape": false,
      "type": "double"
    }
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "arrowFunctions": true,
        "blockBindings": true,
        "forOf": true,
        "modules": true
      },
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "no-console": 0,
      "no-underscore-dangle": 0,
      "no-unused-vars": 1,
      "quotes": 0
    }
  },
  "jshintConfig": {
    "esnext": true,
    "node": true,
    "strict": true,
    "unused": true
  },
  "keywords": [
    "batching",
    "react",
    "strategy",
    "promise",
    "async",
    "generator"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/webcarrot/react-promise-batching/blob/master/LICENSE"
    }
  ],
  "main": "./index.js",
  "maintainers": [
    {
      "email": "admin@webcarrot.pl",
      "name": "webcarrot"
    }
  ],
  "name": "react-promise-batching",
  "peerDependencies": {
    "react-dom": "^15.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webcarrot/react-promise-batching.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "version": "1.0.5"
}
